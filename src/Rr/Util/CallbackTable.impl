namespace Rr {
namespace Util {

template<class Tto, class Tret, class Tfrom, class Targs> 
typename MemberCallable<Tret, Tto, Targs>::CallbackType 
memberCast(Tret (Tfrom::*aFrom)(Targs, ...))
{
	return reinterpret_cast<typename MemberCallable<Tret, Tto, Targs>::CallbackType>(aFrom);
}


}  // namespace Util
}  // namespace Rr
